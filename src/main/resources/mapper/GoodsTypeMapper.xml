<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shopplus.mapper.GoodsTypeMapper">

    <select id="getGoodsListByGoodsTypeId" parameterType="int" resultType="Goods">
        select * from goods where typeId=#{typeId} limit 0,#{size}
    </select>

</mapper>